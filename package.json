{
  "author": "Jan-Niklas Vo√ü",
  "name": "mars-explorer",
  "productName": "mars-explorer",
  "version": "0.0.1",
  "description": "My Electron application description",
  "license": "MIT",
  "main": ".webpack/main",
  "scripts": {
    "postinstall": "ts-node scripts/setup-omnisharp && npx patch-package",
    "start": "cross-env ELECTRON_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier src --write",
    "test": "jest"
  },
  "dependencies": {
    "@codingame/monaco-jsonrpc": "^0.3.1",
    "@fontsource/roboto": "^4.5.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.6.1",
    "electron-log": "^4.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-util": "^0.17.0",
    "express": "^4.15.2",
    "file-uri-to-path": "^2.0.0",
    "fix-path": "^3.0.0",
    "formik": "^2.2.9",
    "fs-extra": "^8.1.0",
    "moment": "^2.29.1",
    "monaco-editor": "^0.26.1",
    "monaco-languageclient": "next",
    "normalize-url": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-use": "^17.2.4",
    "reconnecting-websocket": "^3.2.2",
    "vscode-jsonrpc": "^6.0.0",
    "vscode-languageserver": "7.0.0",
    "ws": "^5.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@electron-forge/cli": "^6.0.0-beta.59",
    "@electron-forge/maker-deb": "^6.0.0-beta.59",
    "@electron-forge/maker-rpm": "^6.0.0-beta.59",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.59",
    "@electron-forge/maker-zip": "^6.0.0-beta.59",
    "@electron-forge/plugin-webpack": "6.0.0-beta.59",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@timfish/webpack-asset-relocator-loader": "^0.1.0",
    "@types/express": "^4.0.35",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "@types/node": "^13.1.7",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.8",
    "@types/vscode": "1.52.0",
    "@types/ws": "0.0.39",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-jest": "^27.0.6",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^0.28.11",
    "download": "^8.0.0",
    "electron": "^13.1.7",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "jest": "^27.0.6",
    "monaco-editor-webpack-plugin": "^4.1.1",
    "node-loader": "^2.0.0",
    "patch-package": "^6.4.7",
    "prettier": "2.3.2",
    "react-refresh": "^0.10.0",
    "remove-files-webpack-plugin": "^1.4.5",
    "style-loader": "^0.20.3",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.1.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.5",
    "webpack": "^4.46.0"
  },
  "resolutions": {
    "vscode-languageserver-types": "3.16.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "extraResource": [
          "./models.json",
          "./resources/examples",
          "./resources/omnisharp"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel"
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack/webpack.main.config.js",
            "renderer": {
              "config": "./webpack/webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/app/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/electron/preload.ts"
                  }
                }
              ]
            },
            "devContentSecurityPolicy": "script-src 'self'"
          }
        ]
      ]
    }
  }
}
